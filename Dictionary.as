package {	public class Dictionary 	{		/*Instance properties*/		// dictionary is a Array of words		var dictionary:Array = ["marvelous","sunshine","pretty","flowers","spring"];		public function Dictionary()		{			//constructor code			randomWord();		}		/**		 * Returns an random word from the dictionary		 **/		public function randomWord():String		{			var i:Number = Math.floor(Math.random() * 4 + 1);			var randomWord:String = dictionary[i];			return randomWord;		}	}}